{
  "name": "habitual_ecommerce_app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "prepare": "husky install",
    "lint": "eslint --quiet . --ext .js,.jsx,.ts,.tsx",
    "pretty": "prettier --write --config .prettierrc.json *.js **/*.{js,ts,tsx}",
    "lint:fix": "eslint --quiet **/**.{ts,tsx} --fix",
    "analyze": "cross-env BUNDLE_ANALYZE=true npm run build",
    "analyze-code": "npm run pretty && npm run lint:fix && npm run lint",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/stack": "^6.1.1",
    "dayjs": "^1.10.7",
    "formik": "^2.2.9",
    "lodash.debounce": "^4.0.8",
    "patch-package": "^6.4.7",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-dotenv": "^3.3.1",
    "react-native-gesture-handler": "^2.2.0",
    "react-native-image-crop-picker": "^0.37.2",
    "react-native-permissions": "^3.2.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.11.1",
    "react-native-svg": "^12.1.1",
    "react-native-toast-message": "^2.1.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.66.4",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.5.1",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
